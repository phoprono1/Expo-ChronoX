{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "react-native-exif"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
